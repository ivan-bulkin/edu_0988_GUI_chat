    <children>
        <TextField fx:id="textField" visible="false"  layoutX="2.0" layoutY="420.0" onAction="#onSendButtonClick" prefHeight="25.0" prefWidth="253.0" />
        <Button fx:id="send" visible="false" layoutX="260.0" layoutY="420.0" onAction="#onSendButtonClick" text="Отправить" />
        <TextArea fx:id="textArea" editable="false" layoutX="2.0" layoutY="8.0" prefHeight="405.0" prefWidth="337.0" wrapText="true" />
        <Button fx:id="connect" visible="true" layoutX="21.0" layoutY="449.0" mnemonicParsing="false" onAction="#connect" prefHeight="25.0" prefWidth="191.0" text="Подключиться" />
    </children>
</AnchorPane>
